# exit X session
super + shift + q
    killall xinit

# lock X session
super + shift + l
    slock

super + Return
    $TERMINAL

super + w
    $BROWSER

super + d
    dmenu_run

# bluetooth menu
super + F1
    dbluez

# bookmarks
# super + n
#     dbm pick
# super + a
#     dbm add

# make screenshot
Print
    sss -sc

