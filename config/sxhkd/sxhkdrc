# exit X session
super + shift + q
    killall xinit

# lock X session
super + l
    slock

super + Return
    $TERMINAL

super + w
    $BROWSER

super + d
    dmenu_run

super + t
    telegram-desktop

super + c
    $TERMINAL -e calcurse

# bluetooth menu
super + F1
    dbluez

# make screenshot
Print
    sshot

