super + shift + q
    killall xinit # exit X session
super + shift + l
    slock # lock X session

super + Return
    $TERMINAL
super + w
    $BROWSER
super + d
    dmenu_run

XF86AudioMute
    lmc toggle-mute
XF86AudioLowerVolume
    lmc down
XF86AudioRaiseVolume
    lmc up
shift + XF86AudioMute
    lmc toggle-pause
shift + XF86AudioLowerVolume
    lmc prev
shift + XF86AudioRaiseVolume
    lmc next
XF86MonBrightnessDown
    colcold
XF86MonBrightnessUp
    colwarm

super + F1
    $TERMINAL -e pulsemixer # audio mixer
super + shift + F1
    $TERMINAL -e ncmpcpp # music player client
super + F10
    bheadphones toggle # toggle bluetooth headphones connection
super + shift + F10
    dbluez # dmenu bluetooth manager

super + shift + w
    dbm open # open bookmark
super + shift + b
    dbm add # add bookmark

Print
    sss -sc # make screenshot

